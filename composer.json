{
    "name": "vismutx/easycredit-api-php",
    "description": "Easycredit api for PHP generated by swagger",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Esycredit api",
            "homepage": "https://www.easycredit-ratenkauf.de/service-integration/inbetriebnahme-e-commerce/api-dokumentation"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Vismutx\\EasycreditApiPhp\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Vismutx\\EasycreditApiPhp\\" : "test/" }
    }
}
